  "phenomic": {
    "CNAME": false,
    "devPort": 3000,
    "source": "content",
    "assets": "assets"
  },
  "config": {
    "validate-commit-msg": {
      "maxSubjectLength": 72,
      "types": "conventional-commit-types"
    }
  },
  "lint-staged": {
    "{src,scripts}/**/*.js": ["eslint --fix", "git add"]
  },
  "babel": {
    "extends": "./babelrc"
  },
