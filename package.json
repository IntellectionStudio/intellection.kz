{
  "name": "intellection",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://intellection.netlify.com",
  "repository": "IntellectionStudio/intellection.kz",
  "license": "GPL-3.0",
  "scripts": {
    "lint:js": "eslint --fix .",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "typecheck": "flow check .",
    "pretest": "npm run lint",
    "test": "npm-run-all --parallel typecheck lint",
    "commitmsg": "validate-commit-msg",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "start": "phenomic start",
    "build": "phenomic build"
  },
  "phenomic": {
    "CNAME": false,
    "devPort": 3000,
    "source": "content",
    "assets": "assets"
  },
  "config": {
    "validate-commit-msg": {
      "maxSubjectLength": 72,
      "types": "conventional-commit-types"
    }
  },
  "lint-staged": {
    "{src,scripts}/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "max-empty-lines": 2,
      "property-no-unknown": [
        true,
        {
          "ignoreProperties": [
            "composes"
          ]
        }
      ]
    }
  },
  "babel": {
    "extends": "./babelrc"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "normalize.css": "^7.0.0",
    "phenomic": "^0.21.1",
    "ramda": "^0.23.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-helmet": "^5.0.3",
    "react-intl": "^2.2.3",
    "react-modal-video": "^1.0.4",
    "react-router": "2.3.0",
    "react-svg-inline": "^2.0.0",
    "react-topbar-progress-indicator": "^1.0.0",
    "recompact": "^3.0.1",
    "redux": "^3.6.0",
    "tachyons": "^4.7.1",
    "warning": "^3.0.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-inline-import": "^2.0.4",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-ramda": "^1.2.0",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.5.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-anvilabs": "^8.2.1",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "flow-bin": "^0.46.0",
    "history": "2.0.0",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-loader": "^2.0.5",
    "postcss-reporter": "^3.0.0",
    "prettier": "^1.3.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^3.0.0-beta",
    "react-redux": "^5.0.4",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "validate-commit-msg": "^2.12.1",
    "webpack": "^2.6.0"
  }
}
