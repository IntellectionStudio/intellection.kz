  "scripts": {
    "lint:js": "eslint --fix .",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint": "npm-run-all --parallel lint:*",
    "test": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "precommit": "lint-staged",
    "prepush": "npm test",
    "start": "phenomic start",
    "build": "phenomic build",
    "production": "http-server dist"
