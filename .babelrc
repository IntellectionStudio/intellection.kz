{
  "presets": [
    "react",
    "env",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "ramda",
    [
      "module-resolver", {
        "root": ["./src"]
      }
    ],
    [
      "lodash", {
        "id": "recompact"
      }
    ]
  ],
  "env": {
    "production": {
      "presets": [
        "react-optimize"
      ]
    }
  }
}
